\mode<presentation>

\definecolor{TWNgreen}{cmyk}{0.9, 0, 1, 0}
% Approximate match for Taiwan colour

\usecolortheme[named=TWNgreen]{structure}%

\setbeamercolor{alerted text}{fg=red}%
\setbeamercolor{example text}{fg=yellow!70!black}%

\setbeamercolor*{palette primary}{fg=TWNgreen!40!black,bg=}%
\setbeamercolor*{palette secondary}{fg=TWNgreen!70!black,bg=white!60!TWNgreen}%
\setbeamercolor*{palette tertiary}{fg=TWNgreen!80!black,bg=TWNgreen!25!white}%
\setbeamercolor*{palette quaternary}{fg=TWNgreen,bg=white!20!TWNgreen}%

\setbeamercolor*{sidebar}{fg=TWNgreen,bg=TWNgreen!75!white}

\setbeamercolor*{palette sidebar primary}{fg=TWNgreen!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=TWNgreen!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=white!10!TWNgreen}

\setbeamercolor*{titlelike}{parent=palette primary}%
\setbeamercolor{frametitle}{bg=}%
\setbeamercolor{frametitle right}{bg=}%

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=red!0,fg=}

%% This part modified from beamercolorthemeorchid.sty
% Copyright 2007 by Till Tantau
\setbeamercolor{block title}{fg=white,bg=blue!40!green}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!75!black}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!75!black}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}
%% end of modification from beamercolorthemeorchid.sty
\mode
<all>
%% end of modification from beamercolorthemespruce.sty
